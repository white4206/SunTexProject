page,
.root {
  background-color: #f2f3f5;
}
.selectPopup {
  padding: 20rpx;
  height: 100%;
  width: 560rpx;
  background-color: #ffffff;
}
.selectPopup .title {
  text-align: center;
  margin-bottom: 20rpx;
}
.selectPopup .btn {
  margin: 20rpx 0;
  text-align: center;
}
.selectPopup .btn button {
  background-color: #337ecc;
  background-image: linear-gradient(90deg, #337ecc 0%, #79bbff 100%);
}
.myCar-box {
  padding-top: 20rpx;
}
.myCar-box .myCar-card .options {
  display: flex;
  justify-content: space-around;
}
.myCar-box .myCar-card .options .optionItem .optionItem-btn {
  background-color: #337ecc;
  background-image: linear-gradient(90deg, #337ecc 0%, #79bbff 100%);
}